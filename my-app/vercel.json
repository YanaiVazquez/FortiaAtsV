{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/menu", "destination": "/index.html" },
    { "source": "/menu/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/", "destination": "/menu/dashboard", "permanent": false }
  ]
}
